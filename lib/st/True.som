True = Boolean (

and: aBlock = (
	^aBlock value
)

asString = (
	^'true'
)

ifFalse: falseAlternativeBlock ifTrue: trueAlternativeBlock = (
	^trueAlternativeBlock value
)

ifFalse: ignoredBlock = (
	^nil
)

ifTrue: aBlock = (
	^aBlock value
)

ifTrue: trueAlternativeBlock ifFalse: falseAlternativeBlock = (
	^trueAlternativeBlock value
)

not = (
	^false
)

or: ignoredBlock = (
	^true
)

printOn: aStream = (
	aStream nextPutAll: 'true'
)

)
