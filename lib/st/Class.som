Class = Behavior (

    "Accessing"
    name = primitive

    "Converting"
    asString = ( ^self name asString )

    "Meta Information"
    superclass = primitive
    fields = primitive
    methods = primitive
    selectors = ( ^self methods collect: [:inv | inv signature ] )

    hasMethod: aSymbol = (
        self methods do: [ :m |
            m signature = aSymbol ifTrue: [ ^true ] ].
        ^false
    )

)
